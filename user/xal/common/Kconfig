source "../../core/Kconfig.ver"

config DEBUG
       bool "Debug"
       default n

config MEMORY_MANAGER
       bool "Memory manager"
	   default y

if MEMORY_MANAGER
config BLOCK_BYTE_ORDER
       int "Block byte order (2^k B per block)"
	   default "6"

endif

menu "Kernel options"
config STACK_KB
       int "Stack size (KB)"
       default "8"

config STACK_EXCPT_KB
       int "Stack size for exceptions (KB)"
       default "1"

endmenu
